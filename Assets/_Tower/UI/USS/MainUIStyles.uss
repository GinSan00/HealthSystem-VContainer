.unity-progress-bar__background {
    width: 100%;
    height: 100%;
    background-color: rgb(0, 0, 0);
}

.unity-progress-bar__container {
    height: auto;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    min-height: 100%;
    max-height: 100%;
}

.unity-progress-bar__progress {
    background-color: rgb(110, 253, 0);
}
